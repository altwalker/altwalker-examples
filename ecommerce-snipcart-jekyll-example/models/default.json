                  "name": "open_cart",
                  "sourceVertexId": "v_product_page",
				  "targetVertexId": "v_cart_open_and_not_empty_navigation",
                  "guard":"global.itemsInCart>0"
                },
              	{
                  "id": "e_open_cart_2",
                  "name": "open_cart",
                  "sourceVertexId": "v_homepage",
				  "targetVertexId": "v_cart_open_and_not_empty_navigation",
                  "guard":"global.itemsInCart>0"
                },
              	{
                  "id": "e_open_cart_3",
                  "name": "do_nothing",
                  "sourceVertexId": "v_product_page_cart_open",
				  "targetVertexId": "v_cart_open_and_not_empty_navigation",
                  "guard":"global.itemsInCart>0"
                },
              	{
                  "id": "e_open_cart_4",
                  "name": "do_nothing",
                  "sourceVertexId": "v_homepage_cart_open",
				  "targetVertexId": "v_cart_open_and_not_empty_navigation",
                  "guard":"global.itemsInCart>0"
                }
              	
			]
		},
        {
          	"name": "CheckoutModel",
			"generator": "random(edge_coverage(100))",
			"vertices": [
              {
                "id":"v_cart_open_and_not_empty_checkout",
                "name":"cart_open_and_not_empty",
