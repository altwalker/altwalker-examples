              {
                "id": "e_place_order",
                "name": "place_order",
                "sourceVertexId": "v_order_confirmation",
                "targetVertexId": "v_order_confirmed",
                "actions": [
                  "global.itemsInCart=0;"
              ]
              },
              {
                "id":"e_go_to_homepage_checkout",
                "name": "go_to_homepage",
